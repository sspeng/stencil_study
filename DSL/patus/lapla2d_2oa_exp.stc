stencil Lap2D2oa (
    double grid A(0 .. x_max+1, 0 .. y_max+1),
    double param alpha,
    double param beta)
{
	domainsize = (1 .. x_max, 1 .. y_max);
  iterate while t < 10000;
	
	operation 
	{
		A[x, y; t+1] =
			alpha * A[x, y; t] +
			beta * (
				A[x+1, y; t] + A[x-1, y; t] +
				A[x, y+1; t] + A[x, y-1; t] 
			);
	}
}
